/*DESCRIPTION:
Complete the solution so that it reverses the string passed into it.

'world'  =>  'dlrow'
'word'   =>  'drow'*/


#include <string>
#include <iostream>

std::string reverseString( std::string str) {
	
	std::cin >> str;

	std::reverse(str.begin(), str.end());

	std::cout << str;

	return str;
}

int main() {

	std::string str;
	reverseString(str);

	return 0;
}
